{
  "restServerPort": 4000,
  "authServerURL": "http://sparky-service:8080",
  "mgmtServerURL": "http://stu-mgmt-backend:3000",
  "cacheDir": "cache/",
  "courses": [
    {
      "courseName": "${SVN_COURSE_NAME}",
      "semester": "${SVN_COURSE_SEMESTER}",
      "repositoryPath": "/repository/${SVN_REPO_NAME}/",
      "accessPath": "/repository/access",
      "svnName": "${SVN_REPO_NAME}",
      "updateStrategy": "IMMEDIATELY",
      "author": "Student Management System",
      "initRepositoryIfNotExists" : true
    }
  ],
  "authUser": "${SVN_MGMT_USER}",
  "authPassword": "${SVN_MGMT_PW}"
}
